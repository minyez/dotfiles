#%Module1.0
#
#     brew install e2fsprogs
#

module-whatis "e2fsprogs@1.46.2 to access Linux file system, installed by Homebrew"

set E2FSPROGS_HOME "/usr/local/Cellar/e2fsprogs/1.46.2"
setenv E2FSPROGS_HOME "$E2FSPROGS_HOME"
prepend-path PATH "$E2FSPROGS_HOME/bin"
prepend-path LIBRARY_PATH "$E2FSPROGS_HOME/lib"
prepend-path PKG_CONFIG_PATH "$E2FSPROGS_HOME/lib/pkgconfig"
#prepend-path LD_LIBRARY_PATH "$TCLTK_HOME/lib"
prepend-path CPATH "$E2FSPROGS_HOME/include"
