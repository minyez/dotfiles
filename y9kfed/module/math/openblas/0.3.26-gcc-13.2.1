#%Module1.0#####################################################################j

set docstr  "Openblas 4.1.2 compiled by GCC 13.2.1"

proc ModulesHelp { } {
        global dotversion
        puts stderr $docstr
}

module-whatis $docstr

set prefix "/opt/packages/openblas/0.3.26/gcc-13.2.1"

setenv       OPENBLAS_HOME    "$prefix"

prepend-path CPATH            "$prefix/include"
prepend-path LIBRARY_PATH     "$prefix/lib64"
prepend-path LD_LIBRARY_PATH  "$prefix/lib64"
prepend-path PKG_CONFIG_PATH  "$prefix/lib64/pkgconfig"
