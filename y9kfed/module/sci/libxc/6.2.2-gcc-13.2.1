#%Module1.0#####################################################################

set docstr "LIBXC 6.2.2 compiled by GCC 13.2.1"

proc ModulesHelp { } {
        global dotversion
        puts stderr $docstr
}

module-whatis      $docstr

set          LIBXC_HOME       "/opt/packages/libxc/6.2.2/gcc-13.2.1"
setenv       LIBXCDIR         "$LIBXC_HOME"
prepend-path PATH             "$LIBXC_HOME/bin"
prepend-path LIBRARY_PATH     "$LIBXC_HOME/lib"
prepend-path LD_LIBRARY_PATH  "$LIBXC_HOME/lib"
prepend-path CPATH            "$LIBXC_HOME/include"
prepend-path PKG_CONFIG_PATH  "$LIBXC_HOME/lib/pkgconfig"
