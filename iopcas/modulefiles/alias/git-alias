#%Module1.0
#
# alias related to Git
#
module-whatis "alias related to Git"

set-alias gst "git status "
set-alias gb "git branch "
set-alias ga "git add "
set-alias gcF "git commit -v -F "
set-alias gd "git diff "
set-alias gdc "git diff --cached "
set-alias grs  "git restore "
set-alias grss "git restore --staged "
